<?xml version="1.0"?>
<robot name="CartMan">

    <!--This model has been redesigned based in work of team TU Delft
        Winners in Amazon Robotics Challenge
        https://github.com/warehouse-picking-automation-challenges/team_acrv_2017.git
        http://juxi.net/projects/AmazonRoboticsChallenge/
    -->

    <link name="base_link">
        <visual>
            <origin rpy="1.570796327 0 0"
                    xyz="0 0 1" />
            <geometry>
                <mesh filename="package://rvizglabre/modelos/cartesian_description/meshes/xAxis.STL" />
            </geometry>
        </visual>

        <!-- Collision For Floor -->
        <collision>
            <origin rpy="0 0 0" xyz="0.0 0.0 -0.05"/>
            <geometry>
                <box size="1.5 1.5 0.01" />
            </geometry>
            <material name="darkred">
                <color rgba=".5 .1 .1 1"/>
            </material>
        </collision>
    </link>

    <link name="link_support1">
    <visual>
        <origin xyz="0.625 0.625 0.970" rpy="0 0 0" />
        <geometry>
          <box size="0.07 0.07 1" />
        </geometry>
        <material name="red">
        <color rgba="0.8 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>


     <joint name="base_support1" type="fixed">
        <parent link="base_link"/>
        <child link="link_support1"/>
        <origin rpy="0 0 0" xyz="0 0 -0.5"/>

    </joint> 

        <link name="link_support2">
    <visual>
        <origin xyz="-0.625 0.625 0.970" rpy="0 0 0" />
        <geometry>
          <box size="0.07 0.07 1" />
        </geometry>
        <material name="red">
        <color rgba="0.8 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>


     <joint name="base_support2" type="fixed">
        <parent link="base_link"/>
        <child link="link_support2"/>
        <origin rpy="0 0 0" xyz="0 0 -0.5"/>

    </joint> 

    <link name="link_support3">
    <visual>
        <origin xyz="0.625 -0.625 0.970" rpy="0 0 0" />
        <geometry>
          <box size="0.07 0.07 1" />
        </geometry>
        <material name="red">
        <color rgba="0.8 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>


     <joint name="base_support3" type="fixed">
        <parent link="base_link"/>
        <child link="link_support3"/>
        <origin rpy="0 0 0" xyz="0 0 -0.5"/>

    </joint> 

        <link name="link_support4">
    <visual>
        <origin xyz="-0.625 -0.625 0.970" rpy="0 0 0" />
        <geometry>
          <box size="0.07 0.07 1" />
        </geometry>
        <material name="red">
        <color rgba="0.8 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>


     <joint name="base_support4" type="fixed">
        <parent link="base_link"/>
        <child link="link_support4"/>
        <origin rpy="0 0 0" xyz="0 0 -0.5"/>

    </joint> 

    <link name="link_support1x">
    <visual>
        <origin xyz="0 0.625 0" rpy="0 0 0" />
        <geometry>
          <box size="1.25 0.07 0.07" />
        </geometry>
        <material name="red">
        <color rgba="0.8 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>


     <joint name="base_support1x" type="fixed">
        <parent link="link_support1"/>
        <child link="link_support1x"/>
        <origin rpy="0 0 0" xyz="0 0 0.5"/>
    </joint> 

    <link name="link_support2x">
    <visual>
        <origin xyz="0 -0.625 0" rpy="0 0 0" />
        <geometry>
          <box size="1.25 0.07 0.07" />
        </geometry>
        <material name="red">
        <color rgba="0.8 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>


     <joint name="base_support2x" type="fixed">
        <parent link="link_support1"/>
        <child link="link_support2x"/>
        <origin rpy="0 0 0" xyz="0 0 0.5"/>
    </joint> 


    <link name="link_support1y">
    <visual>
        <origin xyz="0.625 0 0" rpy="0 0 0" />
        <geometry>
          <box size="0.07 1.25 0.07" />
        </geometry>
        <material name="red">
        <color rgba="0.8 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>


     <joint name="base_support1y" type="fixed">
        <parent link="link_support1"/>
        <child link="link_support1y"/>
        <origin rpy="0 0 0" xyz="0 0 0.5"/>
    </joint> 


        <link name="link_support2y">
    <visual>
        <origin xyz="-0.625 0 0" rpy="0 0 0" />
        <geometry>
          <box size="0.07 1.25 0.07" />
        </geometry>
        <material name="red">
        <color rgba="0.8 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>


     <joint name="base_support2y" type="fixed">
        <parent link="link_support1"/>
        <child link="link_support2y"/>
        <origin rpy="0 0 0" xyz="0 0 0.5"/>
    </joint> 


    <joint name="joint_1" type="prismatic">
        <parent link="base_link" />
        <child link="x_axis_link" />
        <limit effort="1000.0" lower="0"
               upper="0.98" velocity="1.0" />
        <origin rpy="1.570796327 0 0"
                xyz="-0.5 0 1" />
    </joint>

    <link name="x_axis_link">
        <visual>
            <geometry>
                <mesh filename="package://rvizglabre/modelos/cartesian_description/meshes/yAxis.STL" />
            </geometry>
        </visual>

        <collision>
            <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.1 0.08 1.1" />
            </geometry>
            <material name="darkred">
                <color rgba=".5 .1 .1 1"/>
            </material>
        </collision>

    </link>
 

    <joint name="joint_2" type="prismatic">
        <axis xyz="0 0 1"/>
        <parent link="x_axis_link"/>
        <child link="y_axis_link"/>
        <limit effort="1000.0" lower="0"
               upper="0.92" velocity="1.0" />
        <origin rpy="0 0 0"
                xyz="0 0 -0.45" />
    </joint>

    <link name="y_axis_link">
        <visual>
            <geometry>
                <mesh filename="package://rvizglabre/modelos/cartesian_description/meshes/yAxisCarriage.STL" />
            </geometry>
        </visual>

    </link>

    <joint name="joint_3" type="prismatic">
        <axis xyz="0 1 0"/>
        <parent link="y_axis_link"/>
        <child link="z_axis_link"/>
        <limit effort="1000.0" lower="0"
               upper="0.5" velocity="1.0" />
        <origin rpy="0 0 0"
                xyz="0 0.12 0" />
    </joint>

    <link name="z_axis_link">
        <visual>
            <geometry>
                <mesh filename="package://rvizglabre/modelos/cartesian_description/meshes/zAxis.STL"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 -0.038 0" />
        </visual>

<!--         <visual>
            <geometry>
                <mesh filename="package://rvizglabre/modelos/cartesian_description/meshes/yaw.STL"/>
            </geometry>
            <origin rpy="0 0 -1.57" xyz="0 0 0.012"/>
        </visual> -->

    </link>

    <joint name="joint3-tool0" type="fixed">
      <parent link="z_axis_link"/>
      <child link="tool0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </joint>

      <link name="tool0"/>

</robot>
